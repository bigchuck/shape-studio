{
  "dataset_organic_varied": {
    "description": "Dataset generation with maximum variation - for LoRA training",
    "author": "Mr. Chuck",
    "created": "2026-01-06",
    "target_canvas": "MAIN",
    "comments": "Use: BATCH 200 datasets_phase2.json dataset_organic_varied organic_dataset",
    "commands": [
      "CLEAR MAIN ALL",
      {
        "_comment": "Randomize everything for maximum variation",
        "command": "PROC",
        "method": "dynamic_polygon",
        "name": "varied_organic",
        "params": {
          "VERTICES": "RAND(5,12)",
          "BOUNDS": [100, 100, 600, 600],
          "ITERATIONS": "RAND(15,35)",
          "OPERATIONS": "sawtooth,split_offset,squarewave",
          "DIRECTION_BIAS": "random",
          "DEPTH_RANGE": "RAND(0.2,0.5),RAND(0.5,0.9)",
          "WEIGHT_DECAY": "RAND(0.3,0.7)",
          "REMOVE_PROB": "RAND(0.0,0.25)",
          "DISTORT_PROB": "RAND(0.0,0.15)"
        }
      },
      "PROMOTE varied_organic"
    ]
  },

  "dataset_geometric_clean": {
    "description": "Clean geometric shapes with consistent parameters",
    "author": "Mr. Chuck",
    "created": "2026-01-06",
    "target_canvas": "MAIN",
    "comments": "Use: BATCH 100 datasets_phase2.json dataset_geometric_clean geometric_dataset",
    "commands": [
      "CLEAR MAIN ALL",
      {
        "_comment": "Limited randomization for more predictable results",
        "command": "PROC",
        "method": "dynamic_polygon",
        "name": "geometric",
        "params": {
          "VERTICES": "RAND(5,8)",
          "BOUNDS": [150, 150, 550, 550],
          "ITERATIONS": "RAND(8,15)",
          "OPERATIONS": "split_offset",
          "DIRECTION_BIAS": "outward",
          "DEPTH_RANGE": [0.3, 0.6],
          "WEIGHT_DECAY": 0.5
        }
      },
      "PROMOTE geometric"
    ]
  },

