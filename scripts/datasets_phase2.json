  "layered_composition_structured": {
    "description": "Complex multi-layer composition with all structured commands",
    "author": "Mr. Chuck",
    "created": "2026-01-06",
    "comments": "Creates a three-layer composition. All commands in structured format for maximum clarity.",
    "commands": [
      "CLEAR WIP ALL",
      {
        "_comment": "Layer 1: Far background - large, subtle",
        "command": "PROC",
        "method": "dynamic_polygon",
        "name": "far_bg",
        "params": {
          "VERTICES": [12, 16],
          "BOUNDS": [0, 0, 768, 768],
          "ITERATIONS": 40,
          "OPERATIONS": "split_offset",
          "DIRECTION_BIAS": "random",
          "DEPTH_RANGE": [0.1, 0.3]
        }
      },
      {
        "command": "COLOR",
        "name": "far_bg",
        "color": "#ecf0f1"
      },
      {
        "command": "FILL",
        "name": "far_bg",
        "fill": "#f8f9fa"
      },
      {
        "command": "ZORDER",
        "name": "far_bg",
        "z_coord": -20
      },
      {
        "_comment": "Layer 2: Middle ground - medium size, moderate detail",
        "command": "PROC",
        "method": "dynamic_polygon",
        "name": "mid_ground",
        "params": {
          "VERTICES": [8, 10],
          "BOUNDS": [100, 100, 650, 650],
          "ITERATIONS": 25,
          "OPERATIONS": "sawtooth,split_offset",
          "DIRECTION_BIAS": "inward",
          "DEPTH_RANGE": [0.3, 0.6]
        }
      },
      {
        "command": "COLOR",
        "name": "mid_ground",
        "color": "#3498db"
      },
      {
        "command": "FILL",
        "name": "mid_ground",
        "fill": "#85c1e9"
      },
      {
        "command": "ALPHA",
        "name": "mid_ground",
        "alpha": 0.7
      },
      {
        "command": "ZORDER",
        "name": "mid_ground",
        "z_coord": 0
      },
      {
        "_comment": "Layer 3: Foreground - small, detailed, bold",
        "command": "PROC",
        "method": "dynamic_polygon",
        "name": "fore_ground",
        "params": {
          "VERTICES": [5, 7],
          "BOUNDS": [250, 250, 500, 500],
          "ITERATIONS": 15,
          "OPERATIONS": "sawtooth",
          "DIRECTION_BIAS": "outward",
          "DEPTH_RANGE": [0.5, 0.8]
        }
      },
      {
        "command": "COLOR",
        "name": "fore_ground",
        "color": "#e74c3c"
      },
      {
        "command": "FILL",
        "name": "fore_ground",
        "fill": "#f1948a"
      },
